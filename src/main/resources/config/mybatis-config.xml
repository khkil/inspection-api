<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="useGeneratedKeys" value="true" />
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>
    <typeAliases>

        <typeAlias alias="inspection" type="com.example.backend.api.inspection.Inspection"/>
        <typeAlias alias="question" type="com.example.backend.api.inspection.question.model.Question"/>
        <typeAlias alias="answer" type="com.example.backend.api.inspection.question.answer.model.Answer"/>
        <typeAlias alias="member"  type="com.example.backend.api.member.model.Member"/>
        <typeAlias alias="memberAnswer" type="com.example.backend.api.inspection.question.answer.model.MemberAnswer"/>
        <typeAlias alias="result" type="com.example.backend.api.inspection.result.model.Result"/>
        <typeAlias alias="resultContent" type="com.example.backend.api.inspection.result.model.ResultContent"/>
        <typeAlias alias="user" type="com.example.backend.api.user.User"/>
        <typeAlias alias="userAnswer" type="com.example.backend.api.user.UserAnswer"/>
        <typeAlias alias="group" type="com.example.backend.api.group.Group"/>
        <typeAlias alias="progress" type="com.example.backend.api.progress.model.Progress"/>
        <typeAlias alias="rank" type="com.example.backend.api.inspection.rank.Rank"/>
        <typeAlias alias="statistics" type="com.example.backend.api.inspection.result.statistics.Statistics"/>

    </typeAliases>


</configuration>


